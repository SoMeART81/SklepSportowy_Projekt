@model List<SklepSportowy.Models.SprzętSportowy>





<p class="text-center">
    <a asp-controller="SprzętSportowy" asp-action="Add" class="btn btn-primary btn-lg">Dodaj</a>
</p>
<table class="table">

    
    @foreach (var eS in Model)
    {
        <tr>
            
            <td>@eS.NazwaSprzętu</td>
            <td>@eS.ModelSprzętu</td>
            <!-- <td>  <div class="border border-success text-success" style="text-align:center;border-radius: 10px; margin: 5px;"> element. </div>  </td> -->
            <td>@eS.Cena</td>
        </tr>
    }
</table>